{
  "name": "temp-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "prebuild": "yarn format && yarn test",
    "build": "next build && yarn imagemin",
    "build:local": "NODE_ENV=development yarn build",
    "build:dev": "NODE_ENV=development yarn build",
    "build:prod": "NODE_ENV=production yarn build",
    "serve:local": "yarn build:local && serve ./out",
    "serve:dev": "yarn build:dev && serve ./out",
    "serve:prod": "yarn build:prod && serve ./out",
    "format": "prettier --write '**/*.{js,json,ts,tsx}'",
    "lint": "eslint --fix '**/*.{js,ts,tsx}' --ignore-path .gitignore --no-cache",
    "imagemin": "node --loader ts-node/esm ./imagemin.ts",
    "test": "NODE_ENV=test jest",
    "test:update": "yarn test --updateSnapshot",
    "test:watch": "yarn test --watch",
    "analyze": "ANALYZE=true yarn build:prod"
  },
  "dependencies": {
    "next": "^14.2.x",
    "react": "^18",
    "react-dom": "^18",
    "tailwindcss": "3.4.14"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.0.4",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "16.0.1",
    "@types/glob": "8.1.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "10.4.20",
    "destyle.css": "4.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.x",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-unused-imports": "4.1.4",
    "glob": "11.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.0.3",
    "serve": "^14.2.1",
    "sharp": "0.33.5",
    "ts-node": "10.9.2",
    "typescript": "^5"
  }
}
