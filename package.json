{
  "name": "temp-nextjs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build && yarn imagemin",
    "build:local": "NODE_ENV=development yarn build",
    "build:dev": "NODE_ENV=development yarn build",
    "build:prod": "NODE_ENV=production yarn build",
    "serve:local": "yarn build:local && serve ./out",
    "serve:dev": "yarn build:dev && serve ./out",
    "serve:prod": "yarn build:prod && serve ./out",
    "format": "prettier --write '**/*.{js,json,ts,tsx}'",
    "lint": "eslint --fix '**/*.{js.ts,tsx}' --ignore-path .gitignore --no-cache",
    "lint:style": "stylelint '**/*.scss' --fix",
    "imagemin": "node --loader ts-node/esm ./imagemin.ts",
    "analyze": "ANALYZE=true yarn build:prod",
    "lighthouse": "lhci autorun --config=./.lighthouserc.json",
    "lighthouse:local": "rm -rf .lighthouseci_result && yarn lighthouse"
  },
  "dependencies": {
    "next": "^14.2.x",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@lhci/cli": "0.14.0",
    "@next/bundle-analyzer": "14.0.4",
    "@types/imagemin": "9.0.0",
    "@types/imagemin-gifsicle": "7.0.4",
    "@types/imagemin-mozjpeg": "8.0.4",
    "@types/imagemin-pngquant": "8.0.0",
    "@types/imagemin-svgo": "10.0.5",
    "@types/node": "22.5.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "destyle.css": "4.0.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.x",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.4",
    "imagemin": "9.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "sass": "1.78.0",
    "serve": "^14.2.1",
    "stylelint": "16.9.0",
    "stylelint-config-recess-order": "5.1.0",
    "stylelint-config-standard-scss": "13.1.0",
    "ts-node": "10.9.2",
    "typescript": "^5"
  }
}
